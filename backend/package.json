    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "openai": "^5.19.1",
    "prisma": "^6.15.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
